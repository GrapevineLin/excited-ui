{
  "name": "excited-ui",
  "version": "0.0.5-0",
  "type": "module",
  "description": "an excited ui",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-w": "vite build --watch",
    "test": "vitest --config vite.config.ts",
    "test:run": "vitest run --coverage --config vite.config.ts",
    "release": "pnpm publish -f --access public --publish-branch publish"
  },
  "keywords": [
    "excited",
    "vue",
    "ui",
    "vitepress"
  ],
  "author": "Grapevine(YD)Lin",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/excited-ui.cjs",
      "import": "./dist/excited-ui.js"
    },
    "./*": "./*"
  },
  "main": "dist/excited-ui.cjs",
  "module": "dist/excited-ui.js",
  "unpkg": "dist/excited-ui.iife.js",
  "jsdelivr": "dist/excited-ui.iife.js",
  "files": [
    "dist"
  ],
  "types": "dist",
  "devDependencies": {
    "@iconify-json/ic": "^1.1.9",
    "@unocss/core": "^0.45.15",
    "@unocss/reset": "^0.45.15",
    "sass": "^1.54.8",
    "unocss": "^0.45.15",
    "unplugin-vue-components": "^0.22.4"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrapevineLin/excited-ui"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "workspaces": [
    "packages/*"
  ]
}
